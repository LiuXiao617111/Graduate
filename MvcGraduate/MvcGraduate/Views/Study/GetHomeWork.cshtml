<div class="frame_main">
    <ul>
        <li class="frame_title_line">
            <div class="frame_title no_select">我的作业</div>
        </li>

        <li class="detaile_line">
            <table>
                <tbody>
                    <tr class="td_head" height="45">
                        <td>科目</td>
                        <td>作业标题</td>
                        <td>布置时间</td>
                        <td>是否已提交作业</td>
                        <td>操作</td>
                    </tr>
                    @if (Model != null)
                    {
                        foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Subject</td>
                                <td>@item.Title</td>
                                <td>@item.PublishTime</td>
                                <td>@item.State</td>
                                <td>
                                    <a href="javascript:;" onclick="showDetails('homework%2Fhomework_do','SCHOOL_YEAR,TERM,GRADE,CLASS,HOMEWORK_DETAIL,CREATE_TIME,CREATE_USER_NAME,CONTENT,SCORE,COMMENT','作业提交详情','10')">详情</a>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </li>
    </ul>
</div>